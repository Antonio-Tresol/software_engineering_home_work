@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].NombreP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].Apellido1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].Apellido2)
