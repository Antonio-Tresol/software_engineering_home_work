            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].Sexo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].FechaNac)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].Teléfono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].Carne)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante[0].Estado)
            </th>
            <th></th>
        </tr>
